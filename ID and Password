//import package 
import java.util.HashMap;
public class IDandPassword
{
   HashMap<String,String> logInInfo = new HashMap<String,String>();//create a hashmap
   
   //contains the IDs and passwords that will be accepted
   IDandPassword() {
       logInInfo.put("TESTID", "TESTPASSWORD");
       logInInfo.put("AM2311015166", "CC101");
       logInInfo.put("ID", "PASSWORD");
   }
   //method that will return the log in info
   protected HashMap getLogInInfo() {
       return logInInfo;
   }
}
